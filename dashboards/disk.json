{
    "displayName": "GCE VM Instance disk Monitoring",
    "gridLayout": {
      "columns": "2",
      "widgets": [
        {
          "title":"disk utilization for  hdb-appdev-us-central1-a-p1",
          "xyChart":{
             "dataSets":[
                {
                   "timeSeriesQuery":{
                      "timeSeriesQueryLanguage":"fetch gce_instance::agent.googleapis.com/disk/percent_used | filter (metadata.system_labels.name == 'hdb-appdev-us-central1-a-p1') | group_by 1m, [value_percent_used_mean: mean(value.percent_used)] | every 1m"
                   },
                   "plotType":"LINE"
                }
             ],
             "timeshiftDuration":"0s",
             "yAxis":{
                "label":"y1Axis",
                "scale":"LINEAR"
             },
             "chartOptions":{
                "mode":"COLOR"
             }
